@model Model.tb_user
<div class="main-content">
    <div class="main-content-inner">
        <div class="page-content main-content">
            <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="/">首页</a>
                    </li>
                    <li>
                        <a href="/user/userList">客户表</a>
                    </li>
                    <li class="active">
                        客户表详情
</li>
                </ul>
                <!-- /.breadcrumb -->
            </div>

            <div class="row">
                <div class="col-xs-12">
                    <div class="widget-box">
                        <div class="widget-header">
                            <h4 class="widget-title">基本信息</h4>

                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>
                            </div>
                        </div>

                        <div class="widget-body" style="display: block;">
                            <div class="widget-main">
                                	<form class="form-horizontal" role="form">
		<input type="hidden" name="id" value="@Model.id" />
		<div class="form-group">
			<label class="col-xs-2 control-label " >唯一编码</label>
			<div class="col-xs-4"><input type="text" id="code" name="code" class="col-xs-10 col-sm-10 " value="@Model.code"></div>
			<label class="col-xs-2 control-label " >联系人名字</label>
			<div class="col-xs-4"><input type="text" id="nickname" name="nickname" class="col-xs-10 col-sm-10 " value="@Model.nickname"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >联系号码（登陆账号）</label>
			<div class="col-xs-4"><input type="text" id="phonenum" name="phonenum" class="col-xs-10 col-sm-10 " value="@Model.phonenum"></div>
			<label class="col-xs-2 control-label " >地址</label>
			<div class="col-xs-4"><input type="text" id="address" name="address" class="col-xs-10 col-sm-10 " value="@Model.address"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >经度</label>
			<div class="col-xs-4"><input type="text" id="longitude" name="longitude" class="col-xs-10 col-sm-10 " value="@Model.longitude"></div>
			<label class="col-xs-2 control-label " >纬度</label>
			<div class="col-xs-4"><input type="text" id="latitude" name="latitude" class="col-xs-10 col-sm-10 " value="@Model.latitude"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >备注</label>
			<div class="col-xs-4"><input type="text" id="remark" name="remark" class="col-xs-10 col-sm-10 " value="@Model.remark"></div>
			<label class="col-xs-2 control-label " >创建时间</label>
			<div class="col-xs-4"><input type="text" id="createdate" name="createdate" class="col-xs-10 col-sm-10 " value="@Model.createdate"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >更新时间</label>
			<div class="col-xs-4"><input type="text" id="updatedate" name="updatedate" class="col-xs-10 col-sm-10 " value="@Model.updatedate"></div>
			<label class="col-xs-2 control-label " >密码</label>
			<div class="col-xs-4"><input type="text" id="pwd" name="pwd" class="col-xs-10 col-sm-10 " value="@Model.pwd"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >省</label>
			<div class="col-xs-4"><input type="text" id="province" name="province" class="col-xs-10 col-sm-10 " value="@Model.province"></div>
			<label class="col-xs-2 control-label " >市</label>
			<div class="col-xs-4"><input type="text" id="city" name="city" class="col-xs-10 col-sm-10 " value="@Model.city"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >县</label>
			<div class="col-xs-4"><input type="text" id="county" name="county" class="col-xs-10 col-sm-10 " value="@Model.county"></div>
			<label class="col-xs-2 control-label " >所在地</label>
			<div class="col-xs-4"><input type="text" id="location" name="location" class="col-xs-10 col-sm-10 " value="@Model.location"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >性别</label>
			<div class="col-xs-4"><input type="text" id="sex" name="sex" class="col-xs-10 col-sm-10 " value="@Model.sex"></div>
			<label class="col-xs-2 control-label " >职业</label>
			<div class="col-xs-4"><input type="text" id="job" name="job" class="col-xs-10 col-sm-10 " value="@Model.job"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >年龄</label>
			<div class="col-xs-4"><input type="text" id="age" name="age" class="col-xs-10 col-sm-10 " value="@Model.age"></div>
			<label class="col-xs-2 control-label " >星座</label>
			<div class="col-xs-4"><input type="text" id="constellation" name="constellation" class="col-xs-10 col-sm-10 " value="@Model.constellation"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >体重</label>
			<div class="col-xs-4"><input type="text" id="hav" name="hav" class="col-xs-10 col-sm-10 " value="@Model.hav"></div>
			<label class="col-xs-2 control-label " >身高</label>
			<div class="col-xs-4"><input type="text" id="tall" name="tall" class="col-xs-10 col-sm-10 " value="@Model.tall"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >档期</label>
			<div class="col-xs-4"><input type="text" id="schedule" name="schedule" class="col-xs-10 col-sm-10 " value="@Model.schedule"></div>
			<label class="col-xs-2 control-label " >个性签名</label>
			<div class="col-xs-4"><input type="text" id="sgin" name="sgin" class="col-xs-10 col-sm-10 " value="@Model.sgin"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >兴趣爱好</label>
			<div class="col-xs-4"><input type="text" id="hobby" name="hobby" class="col-xs-10 col-sm-10 " value="@Model.hobby"></div>
			<label class="col-xs-2 control-label " >生日</label>
			<div class="col-xs-4"><input type="text" id="birth" name="birth" class="col-xs-10 col-sm-10 " value="@Model.birth"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >微信号</label>
			<div class="col-xs-4"><input type="text" id="wx" name="wx" class="col-xs-10 col-sm-10 " value="@Model.wx"></div>
			<label class="col-xs-2 control-label " >是否单身</label>
			<div class="col-xs-4"><input type="text" id="issingle" name="issingle" class="col-xs-10 col-sm-10 " value="@Model.issingle"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >所在单位</label>
			<div class="col-xs-4"><input type="text" id="company" name="company" class="col-xs-10 col-sm-10 " value="@Model.company"></div>
			<label class="col-xs-2 control-label " >单位地址</label>
			<div class="col-xs-4"><input type="text" id="companyaddress" name="companyaddress" class="col-xs-10 col-sm-10 " value="@Model.companyaddress"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >真实姓名</label>
			<div class="col-xs-4"><input type="text" id="realname" name="realname" class="col-xs-10 col-sm-10 " value="@Model.realname"></div>
		</div>
		<div class="clearfix form-actions"><div class="col-md-offset-5 col-md-9"><button class="btn btn-info" type="button" onclick="save()"><i class="ace-icon fa fa-check bigger-110"></i>Save（保存）</button>
	</form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.page-content -->
</div>
<script>
    function save() {
        var params = $("form").serializeArray();
        $.post("/user/userSave", params, function (data) {
            if (data == "True") {
                alert("保存成功！");
                window.location.href = document.referrer;
            } else {
                //消息
                alert("保存失败！");
            }
        });
        return false;
    }
</script>