@model Model.tb_appusertoken
<div class="main-content">
    <div class="main-content-inner">
        <div class="page-content main-content">
            <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="/">首页</a>
                    </li>
                    <li>
                        <a href="/appusertoken/appusertokenList">App用户登陆Token</a>
                    </li>
                    <li class="active">
                        App用户登陆Token详情
</li>
                </ul>
                <!-- /.breadcrumb -->
            </div>

            <div class="row">
                <div class="col-xs-12">
                    <div class="widget-box">
                        <div class="widget-header">
                            <h4 class="widget-title">基本信息</h4>

                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>
                            </div>
                        </div>

                        <div class="widget-body" style="display: block;">
                            <div class="widget-main">
                                	<form class="form-horizontal" role="form">
		<input type="hidden" name="id" value="@Model.id" />
		<div class="form-group">
			<label class="col-xs-2 control-label " >customer或者serviceUser</label>
			<div class="col-xs-4"><input type="text" id="usertype" name="usertype" class="col-xs-10 col-sm-10 " value="@Model.usertype"></div>
			<label class="col-xs-2 control-label " >生成的加密字符串</label>
			<div class="col-xs-4"><input type="text" id="code" name="code" class="col-xs-10 col-sm-10 " value="@Model.code"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >创建日期</label>
			<div class="col-xs-4"><input type="text" id="addtime" name="addtime" class="col-xs-10 col-sm-10 " value="@Model.addtime"></div>
			<label class="col-xs-2 control-label " >用户表维护表外键id</label>
			<div class="col-xs-4"><input type="text" id="userid" name="userid" class="col-xs-10 col-sm-10 " value="@Model.userid"></div>
		</div>
		<div class="clearfix form-actions"><div class="col-md-offset-5 col-md-9"><button class="btn btn-info" type="button" onclick="save()"><i class="ace-icon fa fa-check bigger-110"></i>Save（保存）</button>
	</form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.page-content -->
</div>
<script>
    function save() {
        var params = $("form").serializeArray();
        $.post("/appusertoken/appusertokenSave", params, function (data) {
            if (data == "True") {
                alert("保存成功！");
                window.location.href = document.referrer;
            } else {
                //消息
                alert("保存失败！");
            }
        });
        return false;
    }
</script>